{{! Alert Template used for both Search and Notification Details Panels }}
<div class="dataminr-alert-search-details dm-jewel-theme-integration">
  {{#if details.trialSearch}}
    <div class="dataminr-trial-search" style="padding: 16px; background-color: rgba(255, 255, 255, 0.05); border-radius: 4px; margin-bottom: 16px;">
      <p style="margin: 0; color: #F5F6F9; font-size: 14px; line-height: 1.5;">
        <svg height="18" width="18" viewBox="0 2 24 24" fill="none">
          <path
            d="M19 9L20.25 6.25L23 5L20.25 3.75L19 1L17.75 3.75L15 5L17.75 6.25L19 9ZM11.5 9.5L9 4L6.5 9.5L1 12L6.5 14.5L9 20L11.5 14.5L17 12L11.5 9.5ZM19 15L17.75 17.75L15 19L17.75 20.25L19 23L20.25 20.25L23 19L20.25 17.75L19 15Z"
            fill="currentColor"
          />
        </svg>
        Pulse has additional AI-powered alerts! <a href="https://www.dataminr.com/products/pulse/cyber-risk/request-a-demo/" target="_blank">Schedule a demo</a> to learn more.
      </p>
    </div>
  {{/if}}
  {{#if (eq details.alerts.length 10)}}
    <div>
      <a href="https://app.dataminr.com/app/dashboard/search" class="p-link">
        More Results in Dataminr (Click `Edit search`)
        {{fa-icon "external-link-square" class="external-link-icon"}}</a>
    </div>
  {{/if}}
  {{#each details.alerts as |alert alertIndex|}}
    <div class="dataminr-alert-detail">
      <div class="dataminr-alert-detail-content">
        <div class="dataminr-alert-detail-header">
          <span
            class="dataminr-alert-detail-type dataminr-alert-detail-type-{{alert.alertTypeNormalized}}"
          >{{alert.alertType}}</span>
          {{#if alert.hasAIContent}}
            <span class="dataminr-alert-ai-icon">
              <svg height="18" width="18" viewBox="0 0 24 24" fill="none">
                <path
                  d="M19 9L20.25 6.25L23 5L20.25 3.75L19 1L17.75 3.75L15 5L17.75 6.25L19 9ZM11.5 9.5L9 4L6.5 9.5L1 12L6.5 14.5L9 20L11.5 14.5L17 12L11.5 9.5ZM19 15L17.75 17.75L15 19L17.75 20.25L19 23L20.25 20.25L23 19L20.25 17.75L19 15Z"
                  fill="currentColor"
                />
              </svg>
            </span>
          {{/if}}
          <span
            class="dataminr-alert-detail-timestamp"
          >{{alert.alertTimestampFormatted}}</span>
          {{#if alert.dataminrAlertUrl}}
            <span class="dataminr-alert-detail-url-icon">
              <a
                href="{{alert.dataminrAlertUrl}}"
                title="View alert in Dataminr"
                target="_blank"
              >
                <svg
                  height="18"
                  width="18"
                  viewBox="0 0 24 24"
                  fill="none"
                  class="sc-jsJBEP kwVdvg"
                >
                  <path
                    d="M19 19H5V5H12V3H5C3.89 3 3 3.9 3 5V19C3 20.1 3.89 21 5 21H19C20.1 21 21 20.1 21 19V12H19V19ZM14 3V5H17.59L7.76 14.83L9.17 16.24L19 6.41V10H21V3H14Z"
                    fill="currentColor"
                  ></path>
                </svg>
              </a>
            </span>
          {{/if}}
          {{#unless (gt details.alerts.length 1)}}
            <span
              class="dataminr-alert-close-icon"
              data-alert-close="true"
              data-alert-id="{{alert.alertId}}"
            >
              <svg
                width="30"
                height="30"
                viewBox="0 0 30 30"
                fill="none"
                xmlns="http://www.w3.org/2000/svg"
              >
                <g clip-path="url(#clip0_453_8834)">
                  <path
                    d="M21.4163 9.876L20.1238 8.5835L14.9997 13.7077L9.87551 8.5835L8.58301 9.876L13.7072 15.0002L8.58301 20.1243L9.87551 21.4168L14.9997 16.2927L20.1238 21.4168L21.4163 20.1243L16.2922 15.0002L21.4163 9.876Z"
                    fill="#F5F6F9"
                  />
                </g>
                <defs>
                  <clipPath id="clip0_453_8834">
                    <rect
                      width="22"
                      height="22"
                      fill="white"
                      transform="translate(4 4)"
                    />
                  </clipPath>
                </defs>
              </svg>
            </span>
          {{/unless}}
        </div>

        {{! Location Section }}
        {{#if alert.estimatedEventLocation.name}}
          <div class="dataminr-alert-detail-location">
            <span class="icon-location">
              <svg height="22" width="22" viewBox="0 0 24 24" fill="none">
                <path
                  d="M12 2C8.13 2 5 5.13 5 9C5 14.25 12 22 12 22C12 22 19 14.25 19 9C19 5.13 15.87 2 12 2ZM12 11.5C10.62 11.5 9.5 10.38 9.5 9C9.5 7.62 10.62 6.5 12 6.5C13.38 6.5 14.5 7.62 14.5 9C14.5 10.38 13.38 11.5 12 11.5Z"
                  fill="currentColor"
                ></path>
              </svg>
            </span>
            <span
              class="dataminr-alert-detail-location-name"
            >{{alert.estimatedEventLocation.name}}</span>
          </div>
        {{/if}}

        {{! Live Brief Section }}
        {{#if alert.liveBrief}}
          <div class="dataminr-alert-live-brief">
            <div class="dataminr-alert-live-brief-regenai-header">
              <div class="dataminr-alert-live-brief-regenai-container">
                <div class="dataminr-alert-live-brief-regenai-left">
                  <div class="dataminr-alert-live-brief-regenai-icon-container">
                    <svg height="24" width="24" viewBox="0 0 24 24" fill="none">
                      <path
                        d="M19 9L20.25 6.25L23 5L20.25 3.75L19 1L17.75 3.75L15 5L17.75 6.25L19 9ZM11.5 9.5L9 4L6.5 9.5L1 12L6.5 14.5L9 20L11.5 14.5L17 12L11.5 9.5ZM19 15L17.75 17.75L15 19L17.75 20.25L19 23L20.25 20.25L23 19L20.25 17.75L19 15Z"
                        fill="currentColor"
                      ></path>
                    </svg>
                  </div>
                  <div class="dataminr-alert-live-brief-regenai-label">
                    <span>ReGenAI</span>
                  </div>
                </div>
                <div class="dataminr-alert-live-brief-actions">
                  <div class="dataminr-alert-live-brief-copy-container">
                    <button
                      class="dataminr-alert-live-brief-copy-btn"
                      data-live-brief-copy="true"
                      data-live-brief-text="{{alert.liveBriefCopyText}}"
                      aria-label="copy_regenai_live_brief"
                      title="Copy Live Brief"
                    >
                      <svg height="24" width="24" viewBox="0 0 24 24" fill="none">
                        <path
                          d="M16 1H4C2.9 1 2 1.9 2 3V17H4V3H16V1ZM19 5H8C6.9 5 6 5.9 6 7V21C6 22.1 6.9 23 8 23H19C20.1 23 21 22.1 21 21V7C21 5.9 20.1 5 19 5ZM19 21H8V7H19V21Z"
                          fill="currentColor"
                        ></path>
                      </svg>
                    </button>
                  </div>
                </div>
              </div>
            </div>
            {{#each alert.liveBrief as |brief|}}
              {{#if (eq brief.version "current")}}
                <div class="dataminr-alert-live-brief-content-section">
                  <div class="dataminr-alert-live-brief-header-row">
                    <div class="dataminr-alert-live-brief-title-row">
                      <div class="dataminr-alert-live-brief-icon-wrapper">
                        <svg height="24" width="24" viewBox="0 0 24 24" fill="none">
                          <path
                            d="M7.41667 13.8333H10.7854L12.1146 12H7.41667V13.8333ZM7.41667 17.5H10V15.6666H7.41667V17.5ZM5.58333 21.1666C5.07917 21.1666 4.64757 20.9871 4.28854 20.6281C3.92951 20.2691 3.75 19.8375 3.75 19.3333V4.66665C3.75 4.16248 3.92951 3.73088 4.28854 3.37185C4.64757 3.01283 5.07917 2.83331 5.58333 2.83331H12.9167L18.4167 8.33331V11.4993L16.5833 10.4993V9.24998H12V4.66665H5.58333V19.3333H11.5L12.5 21.1666H5.58333Z"
                            fill="currentColor"
                          ></path>
                          <path
                            d="M20 15L20.625 13.625L22 13L20.625 12.375L20 11L19.375 12.375L18 13L19.375 13.625L20 15ZM16.25 15.25L15 12.5L13.75 15.25L11 16.5L13.75 17.75L15 20.5L16.25 17.75L19 16.5L16.25 15.25ZM20 18L19.375 19.375L18 20L19.375 20.625L20 22L20.625 20.625L22 20L20.625 19.375L20 18Z"
                            fill="currentColor"
                          ></path>
                        </svg>
                      </div>
                      <span class="dataminr-alert-live-brief-title">{{brief.title}}</span>
                    </div>
                    {{#if brief.timestampFormatted}}
                      <span class="dataminr-alert-live-brief-timestamp">Last updated
                        {{brief.timestampFormatted}}</span>
                    {{/if}}
                  </div>
                  <div class="dataminr-alert-live-brief-summary">
                    <span>{{brief.summary}}</span>
                  </div>
                </div>
              {{/if}}
            {{/each}}
          </div>
        {{/if}}

        {{! Headline }}
        <h3 class="dataminr-alert-detail-headline">{{alert.headline}}</h3>

        {{! Subheadline Section }}
        {{#if alert.subHeadline.title}}
          <div class="dataminr-headline-card-section">
            <div class="dataminr-headline-card-section-header">
              <div>
                <span
                  class="dataminr-headline-card-ods-header-copy"
                >{{alert.subHeadline.title}}</span>
              </div>
            </div>
            {{#if alert.subHeadline.content}}
              <div class="dataminr-message-card">
                <ul class="dataminr-message-card-bullets-list">
                  {{#each alert.subHeadline.content as |contentItem|}}
                    <li>{{contentItem}}</li>
                  {{/each}}
                </ul>
                <div class="dataminr-message-card-tweet-heading">
                  <div>
                    <span class="dataminr-formatted-tweet" dir="auto">
                      <span></span>
                    </span>
                  </div>
                </div>
              </div>
            {{/if}}
          </div>
        {{/if}}

        {{! Public Post Text Section }}
        {{#if alert.publicPost.text}}
          <div class="dataminr-public-post-text">
            <div class="dataminr-public-post-text-header">Excerpt from Public Post:</div>
            <div class="dataminr-public-post-text-content">{{alert.publicPost.text}}</div>
            {{#if alert.publicPost.href}}
              <button
                class="dataminr-public-post-link-btn"
                onclick="window.open('{{alert.publicPost.href}}', '_blank')"
                aria-label="View on Public Post"
                title="{{alert.publicPost.href}}"
              >
                <span class="dataminr-public-post-link-text">View on Public Post</span>
                <svg height="22" width="22" viewBox="0 0 24 24" fill="none">
                  <path
                    d="M19 19H5V5H12V3H5C3.89 3 3 3.9 3 5V19C3 20.1 3.89 21 5 21H19C20.1 21 21 20.1 21 19V12H19V19ZM14 3V5H17.59L7.76 14.83L9.17 16.24L19 6.41V10H21V3H14Z"
                    fill="currentColor"
                  ></path>
                </svg>
              </button>
            {{/if}}
          </div>
        {{/if}}

        {{! Intel Agents Section }}
        {{#if alert.intelAgentsGrouped}}
          <div class="dataminr-alert-intel-agents">
            <div class="dataminr-alert-intel-agents-header">
              <div class="dataminr-alert-intel-agents-header-left">
                <h3 class="dataminr-alert-intel-agents-header-title">
                  <span class="dataminr-alert-intel-agents-header-icon">
                    <svg height="22" width="22" viewBox="0 0 24 24" fill="none">
                      <path
                        d="M10.3096 4.33008L6.58984 6.02051L10.2695 7.69043L10.46 8.03027L11.3096 9.91016C11.0097 9.96014 10.72 10.0203 10.5801 10.2402C10.5701 10.26 10.5407 10.2894 10.54 10.29C10.45 10.46 10.3197 10.5804 10.1797 10.6904C10.1598 10.7102 10.1302 10.7302 10.1104 10.75C10.0204 10.9 9.90027 11.0104 9.78027 11.1104C9.73027 11.1504 9.65039 11.2305 9.65039 11.2305C9.46054 11.4003 9.35974 11.5498 9.34961 11.6797C9.32961 11.8197 9.44004 11.9601 9.54004 12.0801C9.64002 12.2001 9.74019 12.3303 9.7002 12.4902C9.67012 12.64 9.51002 12.6997 9.45996 12.7197L9.40039 12.7402L9.33984 12.7197C9.13002 12.6296 8.98991 12.3001 8.91992 11.9902C8.87995 11.8403 8.49049 11.8796 8.32031 11.8896H8.25C7.89011 11.8896 7.88034 12.03 7.86035 12.2998V12.3604C7.91047 12.7299 8.16042 12.6097 8.36035 12.5098C8.46011 12.4599 8.54984 12.4202 8.58984 12.46C8.6498 12.5299 8.67965 12.6299 8.67969 12.7598V12.7803C8.65982 12.9502 8.75042 13.1099 8.86035 13.2998C8.97035 13.4898 9.08984 13.7205 9.08984 13.9805C9.2598 13.7305 9.50988 13.5401 9.92969 13.54C10.0096 13.54 10.0898 13.5496 10.1797 13.5596C10.2697 13.5296 10.37 13.5205 10.46 13.5205C10.9699 13.5205 11.3104 13.9396 11.6104 14.3096C11.8603 14.6295 12.1 14.9297 12.4199 14.9697C12.4699 14.9597 12.5203 14.96 12.5703 14.96C12.8302 14.9601 13.0705 15.0902 13.2305 15.3301C13.4403 15.6301 13.5096 16.0805 13.3896 16.4404V16.4805L13.3604 16.5098C13.2504 16.6197 13.1203 16.6404 13.0303 16.6504H12.9805V16.6904C13.1601 17.2701 12.8696 17.6405 12.6396 17.9404C12.5198 18.1002 12.3996 18.2499 12.3496 18.4297C12.3196 18.8997 12.0198 19.2198 11.7598 19.5098C11.4398 19.8598 11.1603 20.1602 11.3203 20.7002V20.7998C11.3003 20.8598 11.3203 20.96 11.3203 20.96V20.9502C13.9602 20.6701 16.1903 19.0099 17.2803 16.71L18.29 18.9404C16.57 21.3903 13.7199 23 10.5 23C5.25 23 1 18.73 1 13.5C1.00001 8.27001 5.2202 4.03051 10.4502 4.02051L10.3096 4.33008ZM5.5 8.32031C5.34003 8.32031 5.05012 8.36002 4.87012 8.62988L4.84961 8.66016L4.82031 8.67969C4.78031 8.69969 4.74973 8.71973 4.71973 8.71973C3.64973 10.0097 3 11.68 3 13.5L2.99023 13.5205C2.99049 17.5203 6.12047 20.7898 10.0703 21.0098C9.96035 20.6698 9.92048 20.2796 9.98047 19.9297C9.64054 19.2598 9.66044 18.5402 9.69043 17.7803V17.6299C9.6604 17.37 9.43033 17.1699 9.19043 16.96C8.92043 16.72 8.63031 16.4698 8.57031 16.0898V15.6602C8.55031 15.3602 8.53031 14.9797 8.82031 14.7197C8.83032 14.6199 8.84994 14.51 8.87988 14.4102C8.83993 14.4401 8.79994 14.4502 8.75 14.4502C8.59 14.4502 8.19977 14.0398 8.00977 13.8398L7.83008 13.6602C7.75012 13.5002 7.62017 13.4897 7.40039 13.4697C7.18039 13.4497 6.86973 13.4301 6.71973 13.0801C6.59977 13.01 6.38018 12.7603 6.24023 12.6104C6.18263 12.5431 6.12463 12.485 6.12012 12.4805C5.86012 12.2605 5.52977 11.9798 5.50977 11.5898V11.5498C5.53974 11.3298 5.52976 11.0795 5.50977 10.8096C5.47978 10.3597 5.45048 9.90014 5.65039 9.53027V9.5L5.66992 9.48047C5.85992 9.30047 5.98023 9.03957 5.99023 8.80957C6.00013 8.64978 5.9602 8.49975 5.86035 8.37988C5.74035 8.33988 5.62 8.32031 5.5 8.32031ZM19.5898 12.4102L22 13.5L19.5898 14.5898L18.5 17L17.4102 14.5898L15 13.5L17.4102 12.4102L18.5 10L19.5898 12.4102ZM16.7197 6.32031C17.3397 6.85027 17.8799 7.45995 18.3398 8.12988L17.3203 10.3701C16.7403 9.11012 15.8102 8.02977 14.6602 7.25977L16.7197 6.32031ZM12.9404 5.05957L15 6L12.9404 6.94043L12 9L11.0596 6.94043L9 6L11.0596 5.05957L12 3L12.9404 5.05957ZM21.25 3.75L24 5L21.25 6.25L20 9L18.75 6.25L16 5L18.75 3.75L20 1L21.25 3.75Z"
                        fill="currentColor"
                      ></path>
                    </svg>
                  </span>
                  <span class="dataminr-alert-intel-agents-header-title-text">Intel agents</span>
                </h3>
              </div>
              <div class="dataminr-alert-intel-agents-actions">
                <div class="dataminr-alert-intel-agents-copy-container">
                  <button
                    class="dataminr-alert-intel-agents-copy-btn"
                    data-intel-agents-copy="true"
                    data-intel-agents-text="{{alert.intelAgentsCopyText}}"
                    aria-label="copy_intel_agents"
                    title="Copy Intel Agents"
                  >
                    <svg height="24" width="24" viewBox="0 0 24 24" fill="none">
                      <path
                        d="M16 1H4C2.9 1 2 1.9 2 3V17H4V3H16V1ZM19 5H8C6.9 5 6 5.9 6 7V21C6 22.1 6.9 23 8 23H19C20.1 23 21 22.1 21 21V7C21 5.9 20.1 5 19 5ZM19 21H8V7H19V21Z"
                        fill="currentColor"
                      ></path>
                    </svg>
                  </button>
                </div>
              </div>
            </div>
            <div class="dataminr-alert-intel-agents-container">
              {{#each alert.intelAgentsGrouped as |group|}}
                <div class="dataminr-alert-intel-agents-type-group">
                  <div
                    class="dataminr-alert-intel-agents-type-header"
                  >{{group.typeHeader}}</div>
                  <ul class="dataminr-alert-intel-agents-summary-list">
                    {{#each group.summaries as |summary|}}
                      <li class="dataminr-alert-intel-agents-summary-item">
                        {{#if summary.title}}<strong>{{summary.title}}</strong>{{#if
                            summary.contentText
                          }}:
                            {{summary.contentText}}{{/if}}{{else if
                          summary.contentText
                        }}{{summary.contentText}}{{/if}}
                      </li>
                    {{/each}}
                  </ul>
                </div>
              {{/each}}
              {{#if alert.discoveredEntities}}
                <div class="dataminr-alert-intel-agents-discovered-entities">
                  <div
                    class="dataminr-alert-intel-agents-discovered-entities-header"
                  >Discovered Entities ({{alert.discoveredEntities.length}})</div>
                  <div class="dataminr-alert-intel-agents-discovered-entities-list">
                    {{#each alert.discoveredEntities as |entity|}}
                      <button
                        class="dataminr-alert-intel-agents-discovered-entity-item dataminr-entity-details-trigger"
                        data-entity-name="{{entity.name}}"
                        data-entity-type="{{entity.type}}"
                        data-entity-summary="{{entity.summary}}"
                        data-entity-aliases="{{entity.aliasesFormatted}}"
                        data-entity-url="{{entity.url}}"
                        title="View entity details"
                      >
                        <svg height="22" width="22" viewBox="0 0 24 24" fill="none">
                          <path
                            d="M6.2625 18.0125C7.77083 20.0875 9.68333 21.4167 12 22C14.3167 21.4167 16.2292 20.0875 17.7375 18.0125C19.2458 15.9375 20 13.6333 20 11.1V5L12 2L4 5V11.1C4 13.6333 4.75417 15.9375 6.2625 18.0125ZM16.3 16.6C15.1667 18.25 13.7333 19.35 12 19.9C10.2667 19.35 8.83333 18.25 7.7 16.6C6.56667 14.95 6 13.1167 6 11.1V6.375L12 4.125L18 6.375V11.1C18 13.1167 17.4333 14.95 16.3 16.6ZM11 14V16H13V14H11ZM11 8V12H13V8H11Z"
                            fill="currentColor"
                          ></path>
                        </svg>
                        {{entity.name}}
                      </button>
                    {{/each}}
                  </div>
                </div>
              {{/if}}
            </div>
          </div>
        {{/if}}

        {{! Public Post Media Section }}
        {{#if alert.publicPost.media}}
          <div class="dataminr-public-post-media">
            <div class="dataminr-public-post-media-header">
              <h3 class="dataminr-public-post-media-header-title">Media</h3>
            </div>
            {{#each alert.mediaByType as |mediaGroup|}}
              <div class="dataminr-public-post-media-type-group">
                <h4
                  class="dataminr-public-post-media-type-header"
                >{{mediaGroup.typeHeader}}</h4>
                <div class="dataminr-public-post-media-container">
                  {{#if (or (eq mediaGroup.type "image") (eq mediaGroup.type "photo"))}}
                    {{#if mediaGroup.gridStyle}}
                      <div
                        class="dataminr-public-post-media-image-container"
                        style="{{mediaGroup.gridStyle}}"
                      >
                        {{#each mediaGroup.media as |mediaItem|}}
                          <div class="dataminr-public-post-media-image-item">
                            <img
                              class="dataminr-image-modal-trigger"
                              src="{{mediaItem.href}}"
                              data-image-src="{{mediaItem.href}}"
                            />
                          </div>
                        {{/each}}
                      </div>
                    {{else}}
                      <div class="dataminr-public-post-media-image-container">
                        {{#each mediaGroup.media as |mediaItem|}}
                          <div class="dataminr-public-post-media-image-item">
                            <img
                              class="dataminr-image-modal-trigger"
                              src="{{mediaItem.href}}"
                              data-image-src="{{mediaItem.href}}"
                            />
                          </div>
                        {{/each}}
                      </div>
                    {{/if}}
                  {{else}}
                    {{#each mediaGroup.media as |mediaItem|}}
                      {{#if (eq mediaItem.type "video")}}
                        <div class="dataminr-media-container dataminr-video-container">
                          <video class="dataminr-media-player dataminr-video-player" aria-label="Video player" controls controlslist="nodownload" style="width: 100%;" src="{{mediaItem.href}}" data-media-url="{{mediaItem.href}}" crossorigin="anonymous"></video>
                          <div class="dataminr-media-fallback" style="display: none;">
                            <div class="dataminr-media-fallback-message">
                              <p>Video cannot be embedded due to access restrictions.</p>
                              <a href="{{mediaItem.href}}" target="_blank" rel="noopener noreferrer" class="dataminr-media-fallback-link">
                                Open video in new tab
                                <svg height="16" width="16" viewBox="0 0 24 24" fill="none" style="display: inline-block; vertical-align: middle; margin-left: 4px;">
                                  <path d="M19 19H5V5H12V3H5C3.89 3 3 3.9 3 5V19C3 20.1 3.89 21 5 21H19C20.1 21 21 20.1 21 19V12H19V19ZM14 3V5H17.59L7.76 14.83L9.17 16.24L19 6.41V10H21V3H14Z" fill="currentColor"/>
                                </svg>
                              </a>
                            </div>
                          </div>
                        </div>
                      {{/if}}
                      {{#if (eq mediaItem.type "audio")}}
                        <div class="dataminr-media-container dataminr-audio-container">
                          <audio class="dataminr-audio-player" aria-label="Audio player" controls controlslist="nodownload" style="width: 100%;" src="{{mediaItem.href}}" data-media-url="{{mediaItem.href}}" crossorigin="anonymous"></audio>
                          <div class="dataminr-media-fallback" style="display: none;">
                            <div class="dataminr-media-fallback-message">
                              <p>Audio cannot be embedded due to access restrictions.</p>
                              <a href="{{mediaItem.href}}" target="_blank" rel="noopener noreferrer" class="dataminr-media-fallback-link">
                                Open audio in new tab
                                <svg height="16" width="16" viewBox="0 0 24 24" fill="none" style="display: inline-block; vertical-align: middle; margin-left: 4px;">
                                  <path d="M19 19H5V5H12V3H5C3.89 3 3 3.9 3 5V19C3 20.1 3.89 21 5 21H19C20.1 21 21 20.1 21 19V12H19V19ZM14 3V5H17.59L7.76 14.83L9.17 16.24L19 6.41V10H21V3H14Z" fill="currentColor"/>
                                </svg>
                              </a>
                            </div>
                          </div>
                        </div>
                      {{/if}}
                    {{/each}}
                  {{/if}}
                </div>
              </div>
            {{/each}}
          </div>
        {{/if}}

        {{! Metadata Section }}
        {{#if alert.metadata}}
          <div class="dataminr-alert-metadata">
            <div class="dataminr-alert-metadata-header">
              <h3 class="dataminr-alert-metadata-header-title">Key Points[{{ alert.trailAlert }}]</h3>
              <button
                class="dataminr-alert-metadata-search-btn"
                data-metadata-search="true"
                aria-label="Search Key Points in Polarity"
                title="Search Key Points in Polarity"
              >
                <svg viewBox="0 0 512 512" xmlns="http://www.w3.org/2000/svg" role="img" aria-hidden="true" data-icon="search" data-prefix="fas" class="svg-inline--fa fa-search fa-w-16">
                  <path fill="currentColor" d="M505 442.7L405.3 343c-4.5-4.5-10.6-7-17-7H372c27.6-35.3 44-79.7 44-128C416 93.1 322.9 0 208 0S0 93.1 0 208s93.1 208 208 208c48.3 0 92.7-16.4 128-44v16.3c0 6.4 2.5 12.5 7 17l99.7 99.7c9.4 9.4 24.6 9.4 33.9 0l28.3-28.3c9.4-9.4 9.4-24.6.1-34zM208 336c-70.7 0-128-57.2-128-128 0-70.7 57.2-128 128-128 70.7 0 128 57.2 128 128 0 70.7-57.2 128-128 128z"></path>
                </svg>
              </button>
            </div>
            <div class="dataminr-alert-metadata-content">
              {{#if alert.metadata.threatActors}}
                <div class="dataminr-alert-metadata-section">
                  <span class="dataminr-alert-metadata-label">Threats:</span>
                  <span
                    class="dataminr-alert-metadata-value"
                  >{{alert.metadata.threatActorsFormatted}}</span>
                </div>
              {{/if}}
              {{#if alert.metadata.URL}}
                <div class="dataminr-alert-metadata-section">
                  <span class="dataminr-alert-metadata-label">URLs:</span>
                  <span
                    class="dataminr-alert-metadata-value"
                  >{{alert.metadata.URLFormatted}}</span>
                </div>
              {{/if}}
              {{#if alert.metadata.addresses}}
                <div class="dataminr-alert-metadata-section">
                  <span class="dataminr-alert-metadata-label">Addresses:</span>
                  <span
                    class="dataminr-alert-metadata-value"
                  >{{alert.metadata.addressesFormatted}}</span>
                </div>
              {{/if}}
              {{#if alert.metadata.asOrgs}}
                <div class="dataminr-alert-metadata-section">
                  <span class="dataminr-alert-metadata-label">AS Organizations:</span>
                  <span
                    class="dataminr-alert-metadata-value"
                  >{{alert.metadata.asOrgsFormatted}}</span>
                </div>
              {{/if}}
              {{#if alert.metadata.hashValues}}
                <div class="dataminr-alert-metadata-section">
                  <span class="dataminr-alert-metadata-label">Hashes:</span>
                  <span
                    class="dataminr-alert-metadata-value"
                  >{{alert.metadata.hashValuesFormatted}}</span>
                </div>
              {{/if}}
              {{#if alert.metadata.malware}}
                <div class="dataminr-alert-metadata-section">
                  <span class="dataminr-alert-metadata-label">Malware:</span>
                  <span
                    class="dataminr-alert-metadata-value"
                  >{{alert.metadata.malwareFormatted}}</span>
                </div>
              {{/if}}
            </div>
          </div>
        {{/if}}

        {{! Linked Alerts Section }}
        {{#if alert.linkedAlerts}}
          <div class="dataminr-alert-linked-alerts">
            <div class="dataminr-alert-linked-alerts-header">
              <h3 class="dataminr-alert-linked-alerts-header-title">Event Timeline</h3>
            </div>
            <div class="dataminr-alert-linked-alerts-timeline">
              {{#each alert.linkedAlerts as |linkedAlert|}}
                <div
                  class="dataminr-alert-linked-alerts-item"
                  data-linked-alert-id="{{linkedAlert.alertId}}"
                  aria-label="View alert details"
                  title="View alert details"
                >
                  <div class="dataminr-alert-linked-alerts-item-time-container">
                    <span
                      class="dataminr-alert-linked-alerts-item-time"
                    >{{linkedAlert.alertTimestampFormatted}}</span>
                  </div>
                  <div class="dataminr-alert-linked-alerts-item-content">
                    {{#if linkedAlert.imageUrl}}
                      <div class="dataminr-alert-linked-alerts-item-image-wrapper">
                        <img
                          class="dataminr-alert-linked-alerts-item-image"
                          src="{{linkedAlert.imageUrl}}"
                          alt="Alert media"
                        />
                      </div>
                    {{/if}}
                    <span
                      class="dataminr-alert-linked-alerts-item-headline"
                    >{{linkedAlert.headline}}</span>
                    {{#if linkedAlert.alertTypeFormatted}}
                      <div
                        class="dataminr-alert-linked-alerts-item-type"
                      >{{linkedAlert.alertTypeFormatted}}</div>
                    {{/if}}
                  </div>
                </div>
              {{/each}}
            </div>
          </div>
        {{/if}}

        {{! Reference Terms Section }}
        {{#if alert.alertReferenceTerms}}
          <div class="dataminr-alert-reference-terms">
            <div class="dataminr-alert-reference-terms-header">
              <h3 class="dataminr-alert-reference-terms-header-title">Alert reference
                terms</h3>
            </div>
            <div class="dataminr-alert-reference-terms-container">
              {{#each alert.alertReferenceTerms as |term|}}
                <div class="dataminr-alert-reference-term">{{term.text}}</div>
              {{/each}}
            </div>
          </div>
        {{/if}}

        {{! Footer Section (Companies, Sectors, Topics) }}
        {{#if (or alert.alertCompanies alert.alertSectors alert.alertTopics alert.listsMatched)}}
          <hr />
          {{#if alert.listsMatched}}
            <div class="dataminr-alert-lists-matched">
              <span class="dataminr-alert-lists-matched-label">Lists:</span>
              <span
                class="dataminr-alert-lists-matched-list"
              >{{alert.listsMatchedFormatted}}</span>
            </div>
          {{/if}}
          {{#if alert.alertCompanies}}
            <div class="dataminr-alert-companies">
              <span class="dataminr-alert-companies-label">Companies:</span>
              <span
                class="dataminr-alert-companies-list"
              >{{alert.alertCompaniesFormatted}}</span>
            </div>
          {{/if}}
          {{#if alert.alertSectors}}
            <div class="dataminr-alert-sectors">
              <span class="dataminr-alert-sectors-label">Sectors:</span>
              <span
                class="dataminr-alert-sectors-list"
              >{{alert.alertSectorsFormatted}}</span>
            </div>
          {{/if}}
          {{#if alert.alertTopics}}
            <div class="dataminr-alert-topics">
              <span class="dataminr-alert-topics-label">Topics:</span>
              <span
                class="dataminr-alert-topics-list"
              >{{alert.alertTopicsFormatted}}</span>
            </div>
          {{/if}}
        {{/if}}
        {{#if alert.trialAlert}}
          <div class="dataminr-trial-alert">
            <hr />
            <svg height="18" width="18" viewBox="0 2 24 24" fill="none">
              <path
                d="M19 9L20.25 6.25L23 5L20.25 3.75L19 1L17.75 3.75L15 5L17.75 6.25L19 9ZM11.5 9.5L9 4L6.5 9.5L1 12L6.5 14.5L9 20L11.5 14.5L17 12L11.5 9.5ZM19 15L17.75 17.75L15 19L17.75 20.25L19 23L20.25 20.25L23 19L20.25 17.75L19 15Z"
                fill="currentColor"
              />
            </svg>
            Pulse has additional AI-powered alerts! 
            <a href="https://www.dataminr.com/products/pulse/cyber-risk/request-a-demo/" target="_blank">Schedule a demo</a> to learn more.
          </div>
        {{/if}}
      </div>
      <div class="dataminr-entity-details-container" style="display: none;">
        <div class="dataminr-entity-details-content">
          <header class="dataminr-entity-details-header">
            <section class="dataminr-entity-details-header-left">
              <div class="dataminr-entity-details-title-group">
                <div class="dataminr-entity-details-title-row">
                  <svg height="24" width="24" viewBox="0 0 24 24" fill="none" class="dataminr-entity-details-icon">
                    <path d="M6.2625 18.0125C7.77083 20.0875 9.68333 21.4167 12 22C14.3167 21.4167 16.2292 20.0875 17.7375 18.0125C19.2458 15.9375 20 13.6333 20 11.1V5L12 2L4 5V11.1C4 13.6333 4.75417 15.9375 6.2625 18.0125ZM16.3 16.6C15.1667 18.25 13.7333 19.35 12 19.9C10.2667 19.35 8.83333 18.25 7.7 16.6C6.56667 14.95 6 13.1167 6 11.1V6.375L12 4.125L18 6.375V11.1C18 13.1167 17.4333 14.95 16.3 16.6ZM11 14V16H13V14H11ZM11 8V12H13V8H11Z" fill="currentColor"></path>
                  </svg>
                  <h3 class="dataminr-entity-details-title"></h3>
                </div>
                <span class="dataminr-entity-details-type-badge" id="dataminr-entity-details-type-badge" style="display: none;"></span>
                <div><span class="dataminr-entity-details-about-label" style="display: none;">About</span></div>
              </div>
              <div class="dataminr-entity-details-close-wrapper">
                <button class="dataminr-entity-details-close" aria-label="Back to alert details" name="close">
                  <svg height="24" width="24" viewBox="0 0 24 24" fill="none">
                    <path d="M15.41 7.41L14 6L8 12L14 18L15.41 16.59L10.83 12L15.41 7.41Z" fill="currentColor"/>
                  </svg>
                  <span class="dataminr-entity-details-back-text">Back</span>
                </button>
              </div>
            </section>
          </header>
          <section class="dataminr-entity-details-body">
            <section class="dataminr-entity-details-body-content">
              <div class="dataminr-entity-details-tab-panel">
                <div class="dataminr-entity-details-sections">
                  <div class="dataminr-entity-details-card" id="dataminr-entity-details-summary-section" style="display: none;">
                    <article class="dataminr-entity-details-card-content">
                      <div class="dataminr-entity-details-summary-wrapper">
                        <p class="dataminr-entity-details-summary"></p>
                      </div>
                    </article>
                  </div>
                  <div class="dataminr-entity-details-card" id="dataminr-entity-details-aliases-section" style="display: none;">
                    <article class="dataminr-entity-details-card-content">
                      <h3 class="dataminr-entity-details-card-heading">Aliases</h3>
                      <div class="dataminr-entity-details-card-body">
                        <ul class="dataminr-entity-details-aliases-list"></ul>
                      </div>
                    </article>
                  </div>
                  <div class="dataminr-entity-details-link" id="dataminr-entity-details-link-section" style="display: none;">
                    <a href="#" class="dataminr-entity-details-link-btn" target="_blank" rel="noopener noreferrer" id="dataminr-entity-details-link-btn">
                      View in Dataminr
                      <svg height="22" width="22" viewBox="0 0 24 24" fill="none">
                        <path d="M19 19H5V5H12V3H5C3.89 3 3 3.9 3 5V19C3 20.1 3.89 21 5 21H19C20.1 21 21 20.1 21 19V12H19V19ZM14 3V5H17.59L7.76 14.83L9.17 16.24L19 6.41V10H21V3H14Z" fill="currentColor"/>
                      </svg>
                    </a>
                  </div>
                </div>
              </div>
            </section>
          </section>
        </div>
      </div>
    </div>
  {{/each}}
</div>